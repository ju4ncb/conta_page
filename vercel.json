{
  "version": 2,
  "functions": {
    "api/ssr.ts": {
      "includeFiles": "dist/**, server/**"
    },
    "api/render.ts": {
      "includeFiles": "server/**"
    }
  },
  "routes": [
    { "src": "/.*", "dest": "api/ssr.ts" },
    { "src": "/assets/(.*)", "dest": "dist/client/assets/$1" }
  ]
}
